# UGC AI Orchestration Workspace - Environment Configuration
# Copy this file to .env.local and fill in your API keys

# OpenAI API Key - Used for script generation
# Get from: https://platform.openai.com/api-keys
VITE_OPENAI_API_KEY=sk-...

# KIE.AI Nano Banana API Key - Used for image generation
# Get from: https://www.kie.ai/api/docs
VITE_NANO_BANANA_API_KEY=...

# Google Cloud Vision API Key - Used for quality assurance
# Get from: https://console.cloud.google.com/
VITE_VISION_API_KEY=...

# KIE.AI Veo 3.1 API Key - Used for video generation
# Get from: https://www.kie.ai/api/docs
VITE_VEO_API_KEY=...

# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=...

# Optional: Sentry for error tracking
VITE_SENTRY_DSN=

# Optional: Analytics
VITE_ANALYTICS_ID=

# Optional: Feature flags
VITE_ENABLE_VIDEO_GENERATION=true
VITE_ENABLE_BATCH_PROCESSING=true
VITE_PARALLEL_PROCESSING=true
VITE_MAX_IMAGE_VARIATIONS=3
VITE_QA_THRESHOLD=0.8

# Optional: Cost limits (in USD)
VITE_MAX_PROJECT_COST=50
VITE_WARN_COST_THRESHOLD=40

# Optional: Rate limiting
VITE_IMAGE_BATCH_SIZE=3
VITE_IMAGE_BATCH_DELAY_MS=1000
VITE_QA_BATCH_DELAY_MS=100

# Optional: Timeouts (in milliseconds)
VITE_API_TIMEOUT=30000
VITE_VIDEO_GENERATION_TIMEOUT=300000
